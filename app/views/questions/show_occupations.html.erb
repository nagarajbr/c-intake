	<%# else %>
	<!-- <h1 align="center" >Work Interest Profile</h1> -->
	<%# end %>
<%#else%>
	<!-- <h1 align="center" >Work Assessements</h1> -->
<%# end %>

<%#= @job_zone_desc %>
<%#= @zone_results %>
<%#= debug @zone_results %>
<fieldset>
  <section class="solidbody">
<% count = 0 %>
<p><%= link_to "Back To Work Interests", previous_wizard_path, :class => 'btn btn-default' %>
<%= link_to "Back To General Assessments", page_path(:assessement),class: "btn btn-default" %></p><br>
<% if @job_zone_desc.present? %>
	<% @job_zone_desc.each do |key,val| %>
		<table class="table table-striped table-bordered">
		    <tbody>
					<tr>
						<td> <b> <%= key['name'].split(":").first %> </b> </td>
						<td>  <%= key['name'].split(":").last %> </td>
					</tr>
					<tr>
						<td> <b> Experience </b> </td>
						<td> <%= key['experience'] %> </td>
					</tr>
					<tr>
						<td> <b> Education </b> </td>
						<td> <%= key['education'] %> </td>
					</tr>
					<tr>
						<td> <b> Job Training </b> </td>
						<td> <%= key['job_training'] %> </td>
					</tr>
					<tr>
						<td> <b> Examples </b> </td>
						<td> <%= key['examples'] %> </td>
					</tr>
			</tbody>
		</table>

		<% if @zone_results.present? %>
			<table class="table table-striped table-bordered">
				<thead>
					    <tr>
							<th> Occupation </th>
							<th width="15%"> Median Wage </th>
							<th width="15%"> Wage @ 90% </th>
							<!-- <th width="15%"> Action </th> -->
						</tr>
					  </thead>
			    <tbody>
			    	<% @zone_results.each do |career,val| %>
			    		<tr>
			    			<td> <%= career["occu_title"] %> </td>
							<td> $<%= career["a_median"] %> </td>
							<td> $<%= career["a_pct90"] %> </td>
							<!-- <td><p><u><span><a  href="\pages\career_plan_recommendation_details?name= <%#= career["occu_title"] %>">Details</a></span></u></p></td> -->
						</tr>
					<% end %>
				</tbody>
			</table><br>
		<% else %>
		<h5 class="subheader">No occupations available </h5> <br>
		<% end %>
		<% count = count + 1 %>
	<% end %>


<% end %>
</section>
</fieldset>

